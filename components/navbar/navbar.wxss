/* components/navbar/index.wxss */
/* 顶部要固定定位   标题要居中   自定义按钮和标题要和右边微信原生的胶囊上下对齐 */
@import "/iconfont.wxss";
.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 111111;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 40px;
  justify-content: center;
}
.nav-wrap .nav-back{
  position: absolute;
  left: 24rpx;
  width: 64rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
}
.nav-wrap .nav-title{
  font-size:36rpx;
  color:#333
}


