/* pages/recharge/recharge.wxss */

page {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.agree-modal {
  width: 100%;
  height: 100%;
  overflow: hidden;
  padding: 0 100rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999999999999999999999999999;
  display: flex;
  align-items: center;
}

.agree-agreement {
  background: #fff;
  height: 1000rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 0 24rpx;
  font-size: 32rpx;
  padding-bottom: 50rpx;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.title {
  font-size: 38rpx;
  color: #333;
  font-weight: bold;
  margin-top: 50rpx;
  text-align: center;
}

.desc {
  font-size: 32rpx;
  margin-top: 28rpx;
}

.title1 {
  font-size: 34rpx;
  font-weight: 500;
  margin-top: 28rpx;
  margin-bottom: 22rpx;
  text-align: center;
}

.agree-btn {
  background: linear-gradient(90deg, rgba(91, 222, 130, 1), rgba(31, 211, 145, 1));
  border-radius: 6rpx;
  font-size: 24rpx;
  color: #fff;
  width: 160rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  margin: 40rpx auto;
}

.con {
  padding: 0 24rpx;
  box-sizing: border-box;
}

.member-card {
  height: 324rpx;
  display: flex;
  flex-direction: column;
  background: url('https://betaparkwash.chemi.ren/static/img/wash_nav_bg.png') no-repeat;
  background-size: cover;
  margin-top: 20rpx;
  overflow: hidden;
  border-radius: 10rpx;
}

.card-top {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 24rpx 24rpx;
}

.user-tel {
  font-size: 24rpx;
  color: #fff;
}

.use-record {
  width: 128rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  border: 1rpx solid rgba(255, 255, 255, 1);
  border-radius: 22rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #fff;
}

.card-con {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.card-title {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.money {
  font-size: 72rpx;
  color: #fff;
  font-weight: bold;
}

.card-desc {
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.charge-con {
  padding: 30rpx 24rpx;
  background: #fff;
  margin-top: 20rpx;
  box-shadow: 0px 6px 10px 0px rgba(221, 243, 255, 0.05);
  border-radius: 10rpx;
}

.charge-list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.charge-item {
  width: 194rpx;
  height: 128rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 500;
  border: 1rpx solid #1fd39a;
  border-radius: 10rpx;
  color: #666;
  margin-right: 36rpx;
  box-sizing: border-box;
  position: relative;
}

.charge-item:nth-child(3n) {
  margin-right: 0;
}

.icon {
  background: url(data:image/png;base64,) no-repeat;
  width: 85rpx;
  height: 49rpx;
  background-size: cover;
  position: absolute;
  right: 0;
  top: 0;
}

.promo {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.t-word {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-right: 25rpx;
}


.t-input {
  width: 480rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1rpx solid rgba(166, 166, 166, 1);
  border-radius: 10rpx;
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.t-input input {
  font-size: 24rpx;
  color: #333;
  width: 100%;
}

.item-top {
  font-size: 24rpx;
}

.item-bot {
  font-size: 32rpx;
}

.selected {
  background: #1fd39a;
  border: 1rpx solid transparent;
  color: #fff;
}

.use-title {
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.know-item {
  font-size: 24rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  margin-top: 20rpx;
  line-height: 1.5;
}

.other-money {
  margin-top: 10rpx;
  height: 68rpx;
  border: 1rpx solid #aaa;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  color: #333;
  font-weight: 400;
}

.other-money input {
  width: 315rpx;
  text-align: center;
}

.no-use {
  height: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 0 24rpx;
  background: #fff;
  box-shadow: 0px 6px 10px 0px rgba(221, 243, 255, 0.05);
  border-radius: 10rpx;
  margin-top: 20rpx;
}

.left {
  font-size: 26rpx;
  color: #333;
}

.agreement {
  font-size: 24rpx;
  color: #999;
  font-weight: 400;
  margin-bottom: 20rpx;
  text-align: center;
  height: 24rpx;
  line-height: 24rpx;
}

.bottom-wrap {
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  width: 100%;
}

.bottom {
  width: 100%;
  height: 128rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px -6rpx 10rpx 0px rgba(221, 243, 255, 0.3);
  display: flex;
  flex: row;
  align-items: center;
  justify-content: space-between;
  padding: 0 48rpx;
  box-sizing: border-box;
}

.all {
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(255, 112, 0, 1);
}

.recharge-btn {
  width: 288rpx;
  height: 88rpx;
  background: linear-gradient(45deg, rgba(91, 222, 130, 1), rgba(31, 211, 144, 1));
  line-height: 88rpx;
  text-align: center;
  font-weight: 500;
  background: #1fd390;
  border-radius: 10rpx;
  color: #fff;
  font-size: 32rpx;
}
