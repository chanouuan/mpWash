.pro-close {
  width: 100rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: #fff;
  border: 2rpx solid #ddd;
  margin: 5rpx;
}

.pro-del {
  width: 72rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: #fff;
  border: 2rpx solid #ddd;
  margin: 5rpx;
}

.keyNums {
  overflow: hidden;
  width: 686rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  /* position: absolute;
	bottom: 0;
	left: 0;
	z-index: 10000; */
}

.pro-ok {
  width: 100rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: #fff;
  border: 2rpx solid #ddd;
  margin: 5rpx;
  /* margin-left: 35%; */
}

.pro-d {
  width: 72rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 32rpx;
  text-align: center;
  background-color: #fff;
  border: 2rpx solid #ddd;
  margin: 5rpx;
}

.keyNums .bot .kb-del {
  margin-left: 12rpx;
}

.keyNums .bot .kb-icon {
  width: 72rpx;
  height: 72rpx;
  color: #353535;
  background: #ddd;
  text-align: center;
  box-shadow: 0 1rpx 2rpx 0 #ccd2e3;
  border-radius: 5rpx;
}


